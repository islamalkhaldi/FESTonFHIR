				<element name="Mengde" type="decimal"/>
				<element name="Periode" type="duration"/>
				<element name="Iterasjoner" type="int" minOccurs="0"/>
				<element ref="fs:Dosering" minOccurs="0"/>
			</sequence>
		</complexType>
	</element>
</schema>
